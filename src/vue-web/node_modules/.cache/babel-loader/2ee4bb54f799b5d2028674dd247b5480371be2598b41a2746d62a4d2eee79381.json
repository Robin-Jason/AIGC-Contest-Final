{"ast":null,"code":"import img2048 from '@/assets/GameSelection/2048.png';\nimport imgMemoryCard from '@/assets/GameSelection/卡片记忆.png';\nimport imgHitMouse from '@/assets/GameSelection/打地鼠.png';\nimport imgStoryRelay from '@/assets/GameSelection/故事接龙图片.png';\nimport imgSudoku from '@/assets/GameSelection/数独.png';\nimport imgCrazyNumbers from '@/assets/GameSelection/疯狂的数字.png';\nimport imgSnake from '@/assets/GameSelection/贪吃蛇.png';\nexport default {\n  name: \"GameSelection\",\n  data() {\n    return {\n      activeTab: '所有',\n      games: [{\n        name: '2048',\n        img: img2048\n      }, {\n        name: 'Memory Card',\n        img: imgMemoryCard\n      }, {\n        name: 'Hit Mouse',\n        img: imgHitMouse\n      }, {\n        name: 'Story Relay',\n        img: imgStoryRelay\n      }, {\n        name: 'Sudoku',\n        img: imgSudoku\n      }, {\n        name: 'Crazy Numbers',\n        img: imgCrazyNumbers\n      }, {\n        name: 'Snake',\n        img: imgSnake\n      }]\n    };\n  },\n  computed: {\n    filteredGames() {\n      return this.games.filter(game => game.category.includes(this.activeTab));\n    }\n  },\n  methods: {\n    setActiveTab(tab) {\n      this.activeTab = tab;\n    }\n  }\n};","map":{"version":3,"names":["img2048","imgMemoryCard","imgHitMouse","imgStoryRelay","imgSudoku","imgCrazyNumbers","imgSnake","name","data","activeTab","games","img","computed","filteredGames","filter","game","category","includes","methods","setActiveTab","tab"],"sources":["src/views/GameSelection/GameSelect.vue"],"sourcesContent":["<template>\n  <div class=\"braingame-selection-page\">\n    <div class=\"game-detail\">\n      <img src=\"@/assets/HomeContent/打地鼠.png\" alt=\"打地鼠\" class=\"game-image\" />\n      <div class=\"game-description\">\n        <h2>打地鼠</h2>\n        <p>当游戏开始时，地鼠会随机从洞中冒出。游戏的目标是用锤子打地鼠的头，使地鼠回到洞中，以增加玩家的分数。</p>\n      </div>\n    </div>\n    <div class=\"tabs\">\n      <button :class=\"['tab', activeTab === '所有' ? 'active' : '']\" @click=\"setActiveTab('所有')\">所有</button>\n      <button :class=\"['tab', activeTab === '记忆' ? 'active' : '']\" @click=\"setActiveTab('记忆')\">记忆</button>\n      <button :class=\"['tab', activeTab === '协调' ? 'active' : '']\" @click=\"setActiveTab('协调')\">协调</button>\n      <button :class=\"['tab', activeTab === '推理' ? 'active' : '']\" @click=\"setActiveTab('推理')\">推理</button>\n      <button :class=\"['tab', activeTab === '创造力' ? 'active' : '']\" @click=\"setActiveTab('创造力')\">创造力</button>\n    </div>\n    <div class=\"game-list\">\n      <div v-for=\"game in filteredGames\" :key=\"game.name\" class=\"game-item\">\n        <img :src=\"game.img\" :alt=\"game.name\" />\n        <p>{{ game.name }}</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport img2048 from '@/assets/GameSelection/2048.png';\nimport imgMemoryCard from '@/assets/GameSelection/卡片记忆.png';\nimport imgHitMouse from '@/assets/GameSelection/打地鼠.png';\nimport imgStoryRelay from '@/assets/GameSelection/故事接龙图片.png';\nimport imgSudoku from '@/assets/GameSelection/数独.png';\nimport imgCrazyNumbers from '@/assets/GameSelection/疯狂的数字.png';\nimport imgSnake from '@/assets/GameSelection/贪吃蛇.png';\nexport default {\n  name: \"GameSelection\",\n  data() {\n    return {\n      activeTab: '所有',\n      games: [\n        { name: '2048', img: img2048 },\n        { name: 'Memory Card', img: imgMemoryCard },\n        { name: 'Hit Mouse', img: imgHitMouse },\n        { name: 'Story Relay', img: imgStoryRelay },\n        { name: 'Sudoku', img: imgSudoku },\n        { name: 'Crazy Numbers', img: imgCrazyNumbers },\n        { name: 'Snake', img: imgSnake },\n      ]\n    };\n  },\n  computed: {\n    filteredGames() {\n      return this.games.filter(game => game.category.includes(this.activeTab));\n    }\n  },\n  methods: {\n    setActiveTab(tab) {\n      this.activeTab = tab;\n    }\n  }\n};\n</script>\n\n<style scoped>\n.braingame-selection-page {\n  padding: 20px;\n  background-color: #fbe7e7;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.game-detail {\n  display: flex;\n  align-items: center;\n  margin-bottom: 20px;\n  width: 80%;\n  justify-content: center;\n  padding-bottom: 20px;\n}\n\n.game-image {\n  width: 180px;\n  height: auto;\n  border-radius: 10px;\n  margin-right: 20px;\n}\n\n.game-description {\n  max-width: 600px;\n}\n\n.game-description h2 {\n  font-size: 24px;\n  margin-bottom: 10px;\n}\n\n.game-description p {\n  font-size: 16px;\n}\n\n.tabs {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 20px;\n  width: 80%;\n  padding-bottom: 10px;\n}\n\n.tab {\n  background-color: #ffffff;\n  color: #f08d49;\n  padding: 10px 20px;\n  border: none;\n  cursor: pointer;\n  margin: 0 5px;\n  font-size: 16px;\n  position: relative;\n}\n\n.tab.active {\n  color: #5dc2af;\n}\n\n.tab.active::after {\n  content: '';\n  position: absolute;\n  left: 0;\n  bottom: -10px;\n  width: 100%;\n  height: 3px;\n  background-color: #5dc2af;\n}\n\n.game-list {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  width: 80%;\n}\n\n.game-item {\n  width: 30%;\n  margin: 10px;\n  text-align: center;\n  box-sizing: border-box;\n}\n\n.game-item img {\n  width: 100%;\n  border-radius: 10px;\n}\n\n.game-item p {\n  margin-top: 10px;\n  font-size: 16px;\n}\n</style>"],"mappings":"AA0BA,OAAAA,OAAA;AACA,OAAAC,aAAA;AACA,OAAAC,WAAA;AACA,OAAAC,aAAA;AACA,OAAAC,SAAA;AACA,OAAAC,eAAA;AACA,OAAAC,QAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA,GACA;QAAAH,IAAA;QAAAI,GAAA,EAAAX;MAAA,GACA;QAAAO,IAAA;QAAAI,GAAA,EAAAV;MAAA,GACA;QAAAM,IAAA;QAAAI,GAAA,EAAAT;MAAA,GACA;QAAAK,IAAA;QAAAI,GAAA,EAAAR;MAAA,GACA;QAAAI,IAAA;QAAAI,GAAA,EAAAP;MAAA,GACA;QAAAG,IAAA;QAAAI,GAAA,EAAAN;MAAA,GACA;QAAAE,IAAA;QAAAI,GAAA,EAAAL;MAAA;IAEA;EACA;EACAM,QAAA;IACAC,cAAA;MACA,YAAAH,KAAA,CAAAI,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,QAAA,CAAAC,QAAA,MAAAR,SAAA;IACA;EACA;EACAS,OAAA;IACAC,aAAAC,GAAA;MACA,KAAAX,SAAA,GAAAW,GAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceTyp