ton:hover,\n.more-info-button:hover {\n  background-color: #d9862d;\n}\n\n.weekdays {\n  display: flex;\n  justify-content: space-between;\n  padding: 10px 0;\n  background-color: #e8f5e9;\n  border-radius: 8px;\n}\n\n.day {\n  padding: 10px;\n  border-radius: 50%;\n  background-color: #e0e0e0;\n  text-align: center;\n  font-size: 14px;\n}\n\n.day.active {\n  background-color: #4caf50;\n  color: #fff;\n}\n\n.chat-input {\n  border: 1px solid #ddd;\n  border-radius: 5px;\n  padding: 8px;\n  margin-top: 10px;\n  width: 100%;\n}\n</style>"],"mappings":"AAsGA;EACAA,IAAA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}